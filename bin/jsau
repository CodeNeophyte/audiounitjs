#!/usr/bin/env node

var optimist = require('optimist').usage('Create audio unit template with html ui. $0 [project-name]')
    .check(function(arg){
        if(!(arg._.length === 1))
            throw "You must provide the project name";
    });
    
require('iced-coffee-script');

try {
  require('../lib/jsau')(optimist.argv._[0]);
} catch (err) {
  console.error(err.toString());
}